<%= render :partial => "header" %>


<%= form_for @home, url: {action: "create"}, html: {class: "nifty_form"} do |f| %>
  <label for="">Title</label>
  <%= f.text_field :title %>
  <label for="">Description</label>
  <%= f.text_area :description, size: "60x12" %>
  <%= f.submit "Create" %>
<% end %>
